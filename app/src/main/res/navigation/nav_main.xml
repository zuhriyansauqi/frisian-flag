<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main_nav"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/frisianFlagFragment"
        android:name="com.zuhriyansauqi.frisianflag.page.FrisianFlagFragment"
        android:label="FrisianFlagFragment" />
    <fragment
        android:id="@+id/asiFragment"
        android:name="com.zuhriyansauqi.frisianflag.page.AsiFragment"
        android:label="AsiFragment" />
    <fragment
        android:id="@+id/HPKFragment"
        android:name="com.zuhriyansauqi.frisianflag.page.HPKFragment"
        android:label="HPKFragment" />
    <fragment
