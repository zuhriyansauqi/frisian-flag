            app:destination="@id/frisianFlagFragment" />
        <action
            android:id="@+id/action_homeFragment_to_asiFragment"
            app:destination="@id/asiFragment" />
        <action
            android:id="@+id/action_homeFragment_to_HPKFragment"
            app:destination="@id/HPKFragment" />
        <action
            android:id="@+id/action_homeFragment_to_primamumFragment"
            app:destination="@id/primamumFragment" />
